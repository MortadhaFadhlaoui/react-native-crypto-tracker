{
	"name": "reactnativetdd",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start --dev-client",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"lint:fix": "prettier --config .prettierrc **/*.{ts,tsx,js,json} --write && eslint --fix . --ext .ts,.tsx && tsc --project ./tsconfig.json --noemit",
		"test": "jest __tests__/*",
		"test:watch": "yarn test --watch",
		"test:ci": "yarn test --coverage",
		"e2e:build": "npx detox build -c  android.emu.release",
		"e2e:test": "npx detox test -c  android.emu.release",
		"eas:build:test": "npx eas build --platform=android --profile=test",
		"eas:build:development": "npx eas build --platform=android --profile=development",
		"eas-build-pre-install": "chmod +x eas-hooks/*.sh ; ./eas-hooks/eas-build-pre-install.sh",
		"eas-build-on-success": "./eas-hooks/eas-build-on-success.sh"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint:fix"
		}
	},
	"dependencies": {
		"@react-navigation/native": "^6.1.2",
		"@react-navigation/native-stack": "^6.9.9",
		"@react-navigation/stack": "^6.3.11",
		"@reduxjs/toolkit": "^1.9.2",
		"expo": "~47.0.12",
		"expo-dev-client": "~2.0.1",
		"expo-splash-screen": "~0.17.5",
		"expo-status-bar": "~1.4.2",
		"expo-system-ui": "~2.0.1",
		"firebase": "^9.16.0",
		"react": "^18.2.0",
		"react-dom": "18.1.0",
		"react-native": "0.70.5",
		"react-native-dotenv": "^3.4.7",
		"react-native-gesture-handler": "~2.8.0",
		"react-native-paper": "^5.1.4",
		"react-native-reanimated": "^2.14.4",
		"react-native-safe-area-context": "4.4.1",
		"react-native-screens": "~3.18.0",
		"react-native-svg": "13.4.0",
		"react-native-web": "~0.18.9",
		"react-redux": "^8.0.5",
		"redux": "^4.2.1",
		"redux-saga": "^1.2.2",
		"typescript-fsa": "^3.0.0",
		"typescript-fsa-reducers": "^1.2.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@config-plugins/detox": "^4.0.0",
		"@testing-library/jest-native": "^5.4.0",
		"@testing-library/react-native": "^11.5.0",
		"@types/jest": "^29.4.0",
		"@types/react-native": "~0.70.6",
		"@types/react-redux": "^7.1.25",
		"@types/redux": "^3.6.0",
		"@types/redux-saga": "^0.10.5",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"detox": "^20.1.2",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-react": "^1.1.7",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.0",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-native": "^4.0.0",
		"husky": "^8.0.3",
		"jest": "^29.4.1",
		"jest-expo": "^47.0.1",
		"prettier": "^2.8.1",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.6.3"
	},
	"private": true
}
